{
  "name": "dovetail-cdn-viewer-request",
  "version": "0.0.0",
  "engines": {
    "node": ">= 14.0.0"
  },
  "description": "Viewer request cloudfront function for the Dovetail CDN",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "watch": "jest --watch",
    "lint": "eslint .",
    "build": "yarn run build-sync && yarn run build-prune && yarn run build-zip",
    "build-sync": "rsync -crl --delete --exclude-from .lambdaignore . build",
    "build-prune": "cd build && yarn --production",
    "build-zip": "rm -f build.zip && cd build && zip -rq ../build.zip ."
  },
  "repository": "https://github.com/PRX/dovetail-cdn-viewer-request",
  "author": "ryan cavis <ryan@prx.org>",
  "license": "GPL-3.0-only",
  "private": true,
  "devDependencies": {
    "jest": "^26.6.3",
    "prettier": "^2.3.0"
  }
}
